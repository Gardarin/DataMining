
@{
    ViewBag.Title = "Registration";
}

<h2>Registration</h2>
<div>
    <h3>@ViewBag.Title</h3>
    <form method="post" action="">
        <table>
            <tr>
                <td><p>Введите свое имя </p></td>
                <td><input type="text" name="Name" /> </td>
            </tr>
            <tr>
                <td><p>Введите :</p></td>
                <td>
                    <input type="text" name="Password" />
                </td>
            </tr>
            <tr><td><input type="submit" value="Отправить" /> </td><td></td></tr>
        </table>
    </form>
</div>
